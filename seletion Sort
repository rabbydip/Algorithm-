array=[]
n =int(input("Enter the element of number of array:"))
for i in range(0,n):
     x=input()
     array.append(x)

print("This array:",array)

for i in range(0,n-1):
    for j in range(i+1,n):
        if array[i]>array[j]:
            temp=array[i]
            array[i]=array[j]
            array[j]=temp
        
print("After Sorting:",array)
